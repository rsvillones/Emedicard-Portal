namespace WebUI.Areas.CorporateAdministrator.Controllers
{
    public class ResignedMembersController : BaseAccountController
    {
        public ActionResult ResignedPrincipalMembers(string accountCode)
        {
            var returnValue = base.ValidateAccountCode(accountCode);
            if (returnValue != null) return returnValue;
            return View();

        }

        public ActionResult ResignedDependentMembers(string accountCode)
        {
            var returnValue = base.ValidateAccountCode(accountCode);
            if (returnValue != null) return returnValue;
            return View();
        }
    }
}