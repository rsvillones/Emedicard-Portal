using Corelib;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Net;
using System.Web;
using System.Web.Mvc;

namespace WebUI.Areas.CorporateAdministrator.Controllers
{
    [Authorize(Roles = "SysAd, CanViewAccountInformation")]
    public class AccountInformationController : BaseAccountController
    {
        #region -- Action Results --

        [Authorize(Roles = "SysAd, CanViewAccountInformation")]
        public override ActionResult Index(string accountCode)
        {
            var returnValue = base.Index(accountCode);
            if (returnValue != null) return returnValue;

            return View();
        }

        

        
        #endregion
    }
}