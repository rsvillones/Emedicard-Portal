@model Member
@{
    ViewBag.Title = "Member Profile";
}

<div class="row">
    <div class="col-lg-12 col-centered">
        <div id="panel-title">
            Member Profile
            <input type="button" class="btn btn-primary back btn-sm btn-margin" value="Back to List" onclick="history.go(-1);" />
        </div>
        <div class="panel panel-default">
            <div class="panel-body">
                <div id="panel-container">
                    @Html.Partial("_MemberProfile", Model)
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $('.appliedPlanDescription').hover(
              function () {
                  var tooltip = $(this).parent();
                  tooltip.attr('data-original-title', $(this).val()).tooltip('fixTitle').tooltip('show');
              }, function () {
                  var tooltip = $(this).parent();
                  tooltip.tooltip('hide');
              }
            );
        });
    </script>
}


