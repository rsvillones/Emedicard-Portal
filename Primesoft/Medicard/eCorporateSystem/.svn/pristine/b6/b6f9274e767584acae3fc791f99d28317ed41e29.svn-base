@model Corelib.Models.Question

@{
    ViewBag.Title = "Delete";
}


<div class="row">
    <div class="col-md-6 col-centered">
    <div id="panel-title">Delete Question</div>
        <div class="panel panel-default">
            <div class="panel-body">
                <div id="panel-container">
                    <form class="form-horizontal">
                        <div class="row">
                            <div class="col-lg-12">
                                <label class="text-danger">Are you sure you want to delete this?</label>
                            </div>
                        </div>
                        <div class="row">
                                <div class="col-lg-11 col-centered">
                                    
                                    <div class="form-group">
                                        <label class="control-label col-lg-4">@Html.DisplayNameFor(model => model.Description)</label>
                                        <label class="control-label col-lg-8">@Html.DisplayFor(model => model.Description)</label>
                                        
                                        <label class="control-label col-lg-4">@Html.DisplayNameFor(model => model.DisplayOrder)</label>
                                        <label class="control-label col-lg-8">@Html.DisplayFor(model => model.DisplayOrder)</label>
                                        
                                        <label class="control-label col-lg-4">@Html.DisplayNameFor(model => model.Guid)</label>
                                        <label class="control-label col-lg-8">@Html.DisplayFor(model => model.Guid)</label>
                                        
                                        <label class="control-label col-lg-4"></label>
                                        <label class="control-label col-lg-8"></label>
                                        
                                        <label class="control-label col-lg-4"></label>
                                        <label class="control-label col-lg-8"></label>

                                    </div>
                                </div>
                            </div>
                        <div class="row">
                            <div class="col-lg-12">
                                @using (Html.BeginForm()) {
                                    @Html.AntiForgeryToken()

                                    <div class="form-actions no-color" style="text-align: right">
                                        <input type="submit" value="Delete" class="btn btn-danger" /> 
                                        @Html.ActionLink("Back to List", "Index", null, new {@class = "btn btn-primary"})
                                    </div>
                                }
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

