@model MemberRequest
@{
    ViewBag.Title = "Amendment";
}
@using (Html.BeginForm("Amendments", "RequestManagement", FormMethod.Post, new { accountCode = ViewBag.AccountCode, enctype = "multipart/form-data" }))
{
    @Html.Partial("_Amendment", Model)

    <div class="row space">
        <div class="col-lg-12">
            <div class="no-color pull-right">
                <input type="submit" name="Submit" value="Save" class="btn btn-primary" />
                <input type="submit" name="Submit" value="Submit Request" class="btn btn-primary" />
                @Html.ActionLink("Cancel", "Index", null, new { @class = "btn btn-primary" })
            </div>
        </div>
    </div>
    
}