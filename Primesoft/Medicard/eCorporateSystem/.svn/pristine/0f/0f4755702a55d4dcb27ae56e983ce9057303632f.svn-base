@model Member
@{
    ViewBag.Title = "Member Profile";
}

<div class="row">
    <div class="col-lg-12 col-centered">
        <div id="panel-title">
            Member Profile
            <input action="action" type="button" class="btn btn-primary back btn-sm btn-margin" value="Back to List" onclick="history.go(-1);" />
            @Html.ActionLink("Action Memo", "Create", "ActionMemos", new { memberGuid = Model.Guid }, new { @class = "btn btn-danger back btn-sm btn-margin" })
        </div>
        <div class="panel panel-default">
            <div class="panel-body">
                <div id="panel-container">
                    @Html.Partial("_MemberProfile", Model)
                </div>
            </div>
        </div>
    </div>
</div>


