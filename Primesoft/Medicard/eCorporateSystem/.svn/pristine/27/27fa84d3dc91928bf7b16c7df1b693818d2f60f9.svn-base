<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <header>
        @if (Request.IsAuthenticated)
        {
            <div id="CurrentEmployeeDiv">
                <div id="OuterContainerLeft">
                    <div class="welcometext">
                        <div>
                            <label class="welcome">&nbsp</label>
                            <label class="EmployeeName">Anonymous A. Anony</label>
                            <label class="UserID">ecorpmain@medicardphils.com</label>
                        </div>
                    </div>
                </div>
                <div id="OuterContainerRight">
                    <div class="employeePicture">
                        <div class="dropdown">
                            <img src="~/Content/images/default_employee_pic.png" alt="" class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" />
                            <ul style="right: 0; left: auto;" class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Logout</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        }
        <div class="logo"></div>
        <div class="title"><label>eCorporate Services</label></div>
    </header>

    <div id='cssmenu'>
        @if (Request.IsAuthenticated)
        {
        
            <ul>
                <li class='active has-sub'>
                    <a href='#'><span>Account Manager</span></a>
                    <ul>
                        <li class='has-sub'>
                            <a href='#'><span>Profile Management</span></a>
                            <ul>
                                <li><a href='#'><span>TEST</span></a></li>
                                <li class='last'><a href='#'><span>TEST</span></a></li>
                            </ul>
                        </li>
                        <li class='has-sub'>
                            <a href='#'><span>TEST</span></a>
                            <ul>
                                <li><a href='#'><span>TEST</span></a></li>
                                <li class='last'><a href='#'><span>TEST</span></a></li>
                            </ul>
                        </li>
                    </ul>
                </li>
                <li class='has-sub'><a href='#'><span>Profile Management</span>
                <li><a href='#'><span>User Management</span></a></li>
            </ul>
        
        }
    
        </div>
    <div class="body-content">
        @RenderBody()

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
