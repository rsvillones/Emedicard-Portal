@model IEnumerable<string>

@foreach (var document in Model)
{
    <div class="row form-space">
        <div class="col-lg-12">
            <div class="form-group">
                @Html.ActionLink(document.ToString(), "DownloadFile", new { fileName = document }, new { @onclick=""})
            </div>
        </div>
    </div>
}
