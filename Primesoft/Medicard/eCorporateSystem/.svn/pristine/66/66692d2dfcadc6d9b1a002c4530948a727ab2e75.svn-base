@model IEnumerable<Batch>
<h4 class="form-title">Batch List</h4>
<table class="table table-fixed-left">
    <thead>
        <tr>
            <th style="width: 17%">Account Code</th>
            <th style="width: 20%">Date Uploaded</th>
            <th style="width: 30%">Uploaded By</th>
            <th style="width: 15%">Status</th>

            <th style="width: 17.3333%">&nbsp;</th>
            <th style="width: 0.6666%">&nbsp;</th>
        </tr>
    </thead>
</table>

<div class="scrollable-table-fixed" style="margin-top: -3px !important;margin-bottom: 0 !important">
    <table class="table table-fixed-left">
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td style="width: 17%">
                        @Html.DisplayFor(modelItem => item.AccountCode)
                    </td>
                    <td style="width: 20%">
                        @item.DateUploaded.ToString("MM/dd/yyyy hh:mm tt")
                    </td>
                    <td style="width: 30%">
                        @Html.DisplayFor(modelItem => item.UploadBy)
                    </td>
                    <td style="width: 15%">
                        @Html.DisplayFor(modelItem => item.Status)
                    </td>

                    <td style="width: 18%">
                        <input id="processBatchGuid" type="hidden" value="@item.Guid" />
                        <a id="processBatch" class="btn btn-primary pull-right btn-xs" onclick="processBatch($(this));" href="javascript:void(0);">Process</a>
                        <input id="editBatchGuid" type="hidden" value="@item.Guid" />
                        <a href="javascript:void(0);" onclick="findBatch($(this));" id="findBatch" style="margin-right: 5px" class="btn btn-primary pull-right btn-xs">View Data</a>
                        
                    </td>
                </tr>
            }

        </tbody>
    </table>
</div>