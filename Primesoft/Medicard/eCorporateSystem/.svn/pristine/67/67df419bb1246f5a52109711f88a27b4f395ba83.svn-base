@model CancelledMemberWrapper
@using HtmlHelpers.BeginCollectionItem

@using (Html.BeginCollectionItem("CancelledMembers"))
{
    @Html.HiddenFor(model => model.MemberCode)
    @Html.HiddenFor(model => model.LastName)
    @Html.HiddenFor(model => model.FirstName)
    @Html.HiddenFor(model => model.MiddleName)
    @Html.HiddenFor(model => model.EmailAddress)
    @Html.HiddenFor(model => model.DateOfBirth)
    @Html.HiddenFor(model => model.Area)
    @Html.HiddenFor(model => model.EmployeeNumber)
    @Html.HiddenFor(model => model.AppliedPlan)
    @Html.HiddenFor(model => model.Type)
    @Html.HiddenFor(model => model.Gender)
    @Html.HiddenFor(model => model.CivilStatus)
    @Html.HiddenFor(model => model.Waiver)
    @Html.HiddenFor(model => model.EffectivityDate)
    @Html.HiddenFor(model => model.ValidityDate)
    @Html.HiddenFor(model => model.Remarks)
    @Html.HiddenFor(model => model.EndorsementBatchId)
    @Html.HiddenFor(model => model.DateCancelled);
}
