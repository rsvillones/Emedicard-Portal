@model Member

@{
    ViewBag.Title = "Member Profile";
    Layout = "~/Areas/CorporateAdministrator/Views/Shared/_AccountManagerLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12 col-centered">
        <div id="panel-title">
            Member Profile
            @Html.ActionLink("Approve", "Approve", new { accountCode = Model.AccountCode, guid = Model.Guid }, new { @class = "btn btn-primary btn-xs approve pull-right" })
        </div>
        <div class="panel panel-default">
            <div class="panel-body">
                <div id="panel-container">
                    @Html.Partial("_MemberProfile", Model)
                </div>
            </div>
        </div>
    </div>
</div>


